function squareOrSquareRoot(array) {
  let arr = [];
  let x;
  for (let i = 0; i < array.length; i++) {
    if (Number.isInteger(Math.sqrt(array[i])) === true) {arr.push(Math.sqrt(array[i]));}
    else {arr.push(array[i] ** 2);}  
  }
  return arr;  
}

https://www.codewars.com/kata/5601409514fc93442500010b/solutions/javascript/me/best_practice

function betterThanAverage(classPoints, yourPoints) {
  classPoints.push(yourPoints);
  let sum = 0;
  let count = 0;
  for (let i = 0; i < classPoints.length; i++) {
    sum+=classPoints[i];
    count++;
  } 
  return (yourPoints > sum / count) ? true : false;
}

function elevator(left, right, call){
  if (left === right && right === call) return 'right';
  if (Math.abs(left - call) === Math.abs(right - call)) return 'right';
  if (Math.abs(left - call) > Math.abs(right - call)) return 'right';
  if (Math.abs(left - call) < Math.abs(right - call)) return 'left';
}

function getSum( a,b ) {
  if (a === b) return a;
  let sum = 0;
  let higher;
  let lower;
  if (a > b) {
   higher = a;
   lower = b;
  } else {
     higher = b;
     lower = a;
    }
 
 for (let i = lower; i <= higher; i++){
       sum += i;
 }
  return sum;
}
